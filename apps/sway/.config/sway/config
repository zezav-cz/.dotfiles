include /etc/sway/outputs.d/*.conf
include /etc/sway/inputs.d/*.conf
include $HOME/.config/sway/outputs.d/*.conf
include $HOME/.config/sway/inputs.d/*.conf
include /etc/sway/modes/*.conf
include $HOME/.config/sway/config.d/*.conf
bar {
  swaybar_command waybar
}
